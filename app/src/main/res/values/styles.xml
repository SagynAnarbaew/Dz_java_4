<resources xmlns:tools="http://schemas.android.com/tools">
    <style name="ButtonStyle" parent="Widget.MaterialComponents.Button">
        <item name="materialThemeOverlay">@style/GreenButtonBlueThemeOverlay</item>
        <item name="android:layout_height">60dp</item>
        <item name="colorPrimary">@color/purple_200</item>
        <item name="colorAccent">@color/purple_700</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:letterSpacing">0.0</item>
        <item name="android:textSize">16sp</item>
        <item name="android:layout_marginStart">12dp</item>
        <item name="android:layout_marginEnd">12dp</item>
        <item name="cornerRadius">56dp</item>
        <item name="android:textStyle">normal</item>
    </style>
    <style name="GreenButtonBlueThemeOverlay">
        <item name="colorPrimary">#71B8F1</item>
        <item name="colorPrimary" tools:ignore="DuplicateDefinition">#DF316C</item>
    </style>
</resources>